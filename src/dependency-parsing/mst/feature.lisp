(defun map-length (len)
  (cond ((<  len  0)  "-1")
        ((=  len  0)   "0")
        ((<= len  1)   "1")
        ((<= len  2)   "2")
        ((<= len  4)   "4")
        ((<= len  8)   "8")
        ((<= len 16)  "16")
        ((<= len 32)  "32")
        (t           "33+")))

